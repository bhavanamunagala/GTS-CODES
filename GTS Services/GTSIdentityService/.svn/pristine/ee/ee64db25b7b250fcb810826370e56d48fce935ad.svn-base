create table gts_users(
gts_user_id                bigint,
gts_user_email             varchar(50)  not null,
gts_user_password          varchar(50)  not null,
gts_user_status            bit         not null,
gts_user_login_tries       tinyint     not null,
gts_primary_contact_number  int        default null,
gts_user_country_code       tinyint    default null,
gts_primary_contact_is_validated  bit  not null,
gts_user_email_is_validated    bit     not null,
primary key(gts_user_id),
unique (gts_user_email),
unique (gts_user_country_code)
);


